{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "CRICKET_API_KEY": "@cricket_api_key",
    "CRICKET_API_BASE_URL": "@cricket_api_base_url"
  }
}
